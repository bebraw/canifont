{
  "name": "canifont",
  "version": "0.0.2",
  "description": "Figure out what fonts to use",
  "bin": "bin/index.js",
  "main": "dist/index.js",
  "scripts": {
    "sync": "webpack-defaults",
    "prebuild": "npm run clean:dist",
    "build": "cross-env NODE_ENV=production babel -s true src -d dist --ignore 'src/**/*.test.js'",
    "clean:dist": "del-cli dist",
    "start": "npm run serve:dev src",
    "serve:dev": "nodemon $2 --exec babel-node",
    "lint": "eslint --cache src test",
    "security": "nsp check",
    "pretest": "npm run lint",
    "test": "jest",
    "test:coverage": "jest --collectCoverageFrom='src/**/*.js' --coverage",
    "travis:test": "yarn run test",
    "travis:coverage": "yarn run test:coverage",
    "travis:lint": "yarn run lint && yarn run nsp",
    "lint-staged": "lint-staged"
  },
  "keywords": [],
  "author": "Juho Vepsäläinen",
  "license": "MIT",
  "dependencies": {
    "browserslist": "^1.7.5",
    "caniuse-api": "^1.5.3"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-jest": "^19.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-webpack": "^1.0.0",
    "cross-env": "^3.1.4",
    "del-cli": "^0.2.1",
    "eslint": "^3.16.1",
    "eslint-config-webpack": "^1.0.0",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^19.0.2",
    "lint-staged": "^3.3.1",
    "nodemon": "^1.11.0",
    "nsp": "^2.6.2",
    "pre-commit": "^1.2.2",
    "webpack-defaults": "0.0.1"
  },
  "jest": {
    "testMatch": [
      "**/*-test.js"
    ]
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
